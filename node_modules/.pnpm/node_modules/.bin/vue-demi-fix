#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules/vue-demi/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules/vue-demi/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vue-demi/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vue-demi/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules/vue-demi/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules/vue-demi/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vue-demi@0.14.5_vue@3.3.4/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vue-demi/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vue-demi/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
fi
