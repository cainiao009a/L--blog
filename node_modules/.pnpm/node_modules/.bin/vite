#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules/vite/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules/vite/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vite/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vite/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules/vite/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules/vite/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+vite@4.3.9/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vite/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules/vite/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
