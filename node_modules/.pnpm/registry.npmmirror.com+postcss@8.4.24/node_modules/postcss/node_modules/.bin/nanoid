#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+postcss@8.4.24/node_modules/postcss/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+postcss@8.4.24/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/bin/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+nanoid@3.3.6/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules:/mnt/e/blog/node_modules:/mnt/e/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+postcss@8.4.24/node_modules/postcss/node_modules:/mnt/e/blog/vitepress-demo-mondaylab/node_modules/.pnpm/registry.npmmirror.com+postcss@8.4.24/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+nanoid@3.3.6/node_modules/nanoid/bin/nanoid.cjs" "$@"
fi
